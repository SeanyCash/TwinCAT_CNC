    <Method Name="PRG_Null" Id="{95052e96-06a1-4162-a970-9a7ef220b3e3}">
      <Declaration><![CDATA[METHOD PRG_Null : BOOL
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[nHomeAllSeq:=0;
nHomeXSeq:=0;
nHomeYSeq:=0;
nHomeZSeq:=0;
bProgramDone:= FALSE;
bAxisEnable:= FALSE;

/////////////////////////////////]]></ST>
