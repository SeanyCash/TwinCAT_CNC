    <Method Name="ProgramLoader" Id="{89f9fb87-c1f3-4063-b9ac-059ba224bd26}">
      <Declaration><![CDATA[METHOD ProgramLoader : BOOL
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF bProgramDone THEN
	
	ProgramCall := eNull;

END_IF

CASE ProgramCall OF	
	
	eNull:
	
		PRG_Null();

	eHomeAll:
	
		bProgramDone:=FALSE;
		PRG_HomeAll();
	
	eHomeX:
	
		bProgramDone:=FALSE;
		PRG_HomeX();
	
	eHomeY:
	
		bProgramDone:=FALSE;
		PRG_HomeY();	
	
	eHomeZ:
	
		bProgramDone:=FALSE;
		PRG_HomeZ();

END_CASE

]]></ST>
      </Implementation>
    </Method>
