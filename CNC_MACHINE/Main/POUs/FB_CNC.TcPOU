  <POU Name="FB_CNC" Id="{aa8e508b-b3d9-4f69-bd41-818be025bca8}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK FB_CNC
VAR_INPUT
END_VAR
VAR_OUTPUT
END_VAR
VAR
	mcBuildGroup	: CfgBuildExt3DGroup;	
	mcGearIn		: MC_GearIn;
	mcGearOut		: MC_GearOut;

	X_Motion		: FB_MotionFunctions;
	X2_Motion 		: FB_MotionFunctions;
	Y_Motion		: FB_MotionFunctions;
	Z_Motion		: FB_MotionFunctions;
	
	
	X_MC_IN			: ST_Motion_Inputs;
	X2_MC_IN		: ST_Motion_Inputs;
	Y_MC_IN			: ST_Motion_Inputs;
	Z_MC_IN			: ST_Motion_Inputs;
	
	X_MC_OUT			: ST_Motion_Outputs;
	X2_MC_OUT			: ST_Motion_Outputs;
	Y_MC_OUT			: ST_Motion_Outputs;
	Z_MC_OUT			: ST_Motion_Outputs;
	
	
	X_Ref	: AXIS_REF;
	X2_Ref	: AXIS_REF;
	Y_Ref	: AXIS_REF;
	Z_Ref	: AXIS_REF;
	
	
	i_bXHomingSwitch		AT %I* : BOOL;
	i_bX2HomingSwitch		AT %I* : BOOL;
	i_bYHomingSwitch		AT %I* : BOOL;
	i_bZHomingSwitch		AT %I* : BOOL;
	
	bXHomingSwitch		: BOOL;
	bX2HomingSwitch		: BOOL;
	bYHomingSwitch		: BOOL;
	bZHomingSwitch		: BOOL;
	
	q_XHomingPWR			AT %Q* : BOOL;
	q_X2HomingPWR			AT %Q* : BOOL;
	q_YHomingPWR			AT %Q* : BOOL;
	q_ZHomingPWR			AT %Q* : BOOL;
	q_XLimitPWR				AT %Q* : BOOL;
	q_YLimitPWR				AT %Q* : BOOL;
	
	bHomeX		: BOOL;
	bHomeX2		: BOOL;
	bHomeY		: BOOL;
	bHomeZ		: BOOL;
	
	
	i_bXLimitSwitch			AT %I* : BOOL;
	i_bYLimitSwitch			AT %I* : BOOL;
	bXLimitSwitch			: BOOL;
	bYLimitSwitch			: BOOL;
	
	nHomeAllSeq	: INT;
	nHomeXSeq	: INT;
	nHomeYSeq	: INT;
	nHomeZSeq	: INT;
	
	ProgramCall	: E_Programs;
	

END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[// Motion Calls

X_Motion( 
			Ref:= X_Ref,
		  	Motion_In := X_MC_IN,
		  	Motion_Out => X_MC_OUT,
			bHomingInput := bXHomingSwitch,
			bLimitSwitch := bXLimitSwitch,
			bHomeExecute := bHomeX
										);
							
X2_Motion( 
			Ref:= X2_Ref,
		  	Motion_In := X2_MC_IN,
		  	Motion_Out => X2_MC_OUT,
			bHomingInput := bX2HomingSwitch,
			bLimitSwitch := bXLimitSwitch,
			bHomeExecute := bHomeX2
										);
						
Y_Motion( 
			Ref:= Y_Ref,
		  	Motion_In := Y_MC_IN,
		  	Motion_Out => Y_MC_OUT,
			bHomingInput := bYHomingSwitch,
			bLimitSwitch := bYLimitSwitch,
			bHomeExecute := bHomeY
										);
							
Z_Motion( 
			Ref:= Z_Ref,
		  	Motion_In := Z_MC_IN,
		  	Motion_Out => Z_MC_OUT ,
			bHomingInput := bZHomingSwitch,
			bHomeExecute := bHomeZ
										);
										

ProgramLoader();
NCI();
Inputs();
Outputs();]]></ST>
    </Implementation>
    <Method Name="Inputs" Id="{44930261-f7b3-46c2-ba50-0ddd8b114d05}">
      <Declaration><![CDATA[METHOD Inputs : BOOL
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[// Homing Switch Inputs

bXHomingSwitch	:= i_bXHomingSwitch;	
bX2HomingSwitch	:= i_bX2HomingSwitch;	
bYHomingSwitch	:= i_bYHomingSwitch;	
bZHomingSwitch	:= i_bZHomingSwitch;

// Limit Switch Inputs

bXLimitSwitch	:= i_bXLimitSwitch;
bYLimitSwitch	:= i_bYLimitSwitch;	 ]]></ST>
      </Implementation>
    </Method>
    <Method Name="NCI" Id="{dc9731e6-e261-44f3-b6f6-41806c4884e1}">
      <Declaration><![CDATA[METHOD NCI : BOOL
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[// bBuild3DGroup:= bFirstScan;

mcBuildGroup(
	bExecute:= bBuild3DGroup , 
	nGroupId:= 5, 
	nXAxisId:= 1, 
	nYAxisId:= 3, 
	nZAxisId:= 4, 
	nQ1AxisId:= , 
	nQ2AxisId:= , 
	nQ3AxisId:= , 
	nQ4AxisId:= , 
	nQ5AxisId:= , 
	tTimeOut:= , 
	bBusy=> , 
	bErr=> , 
	nErrId=> );
	
	
mcGearIn(
	Master:= X_Ref , 
	Slave:= X2_Ref, 
	Execute:= bGearInExecute AND NOT InGearComplete, 
	RatioNumerator:= 1, 
	RatioDenominator:= 1, 
	Acceleration:= , 
	Deceleration:= , 
	Jerk:= , 
	BufferMode:= , 
	Options:= , 
	InGear=> InGearComplete, 
	Busy=> , 
	Active=> , 
	CommandAborted=> , 
	Error=> , 
	ErrorID=> );
	
mcGearOut(
	Slave:= X2_Ref, 
	Execute:= bGearOutExecute AND NOT GearOutDone, 
	Options:= , 
	Done=> GearOutDone, 
	Busy=> , 
	Error=> , 
	ErrorID=> );
	
	
	
	
IF InGearComplete THEN
	
	bGearInExecute:=FALSE;

END_IF]]></ST>
      </Implementation>
    </Method>
    <Method Name="Outputs" Id="{bfb1dda8-5611-458d-bc52-800eff2f8798}">
      <Declaration><![CDATA[METHOD Outputs : BOOL
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[q_XHomingPWR 	:= TRUE;	
q_X2HomingPWR 	:= TRUE;
q_YHomingPWR	:= TRUE;
q_ZHomingPWR	:= TRUE;]]></ST>
      </Implementation>
    </Method>
    <Method Name="PRG_HomeAll" Id="{dab3d8ef-3b5b-4130-bf86-a872d94b2fd9}">
      <Declaration><![CDATA[METHOD PRG_HomeAll : BOOL
VAR_INPUT
END_VAR

VAR

END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[


CASE nHomeAllSeq OF
		 		

	0:
		bProgramDone := FALSE;
		bAxisEnable := TRUE;
        IF X_MC_OUT.bPowerStatus AND X2_MC_OUT.bPowerStatus THEN
            bGearInExecute := TRUE;
            IF InGearComplete THEN
            nHomeAllSeq :=10;	 
		    END_IF
		 END_IF
	10:
		// Home both geared axes first
		IF X_MC_OUT.bPowerStatus AND X2_MC_OUT.bPowerStatus THEN
			bHomeX := TRUE;
			nHomeAllSeq:=20;	
		ELSE
			nHomeAllSeq:=0;
		END_IF
	20:
		IF X_MC_OUT.bAxisHomed THEN
			bHomeX:= FALSE;
            bGearInExecute:=FALSE;
            nHomeAllSeq:=30;
		END_IF
	30:
		bGearOutExecute := TRUE;
		IF X_MC_OUT.bPowerStatus AND X2_MC_OUT.bPowerStatus THEN
            bHomeX := TRUE;
            bHomeX2 := TRUE;
            nHomeAllSeq := 40;
		END_IF
	40:
		IF X_MC_OUT.bAxisHomed THEN
			bHomeX := FALSE;		
		END_IF
        IF X2_MC_OUT.bAxisHomed THEN
            bHomeX2 := FALSE;
		END_IF
        IF X_MC_OUT.bAxisHomed AND X2_MC_OUT.bAxisHomed THEN
            nHomeAllSeq:=50;
		END_IF
		
	50:
	
		bGearOutExecute := FALSE;
        bGearInExecute := TRUE;
        IF InGearComplete THEN
            bAxisEnable := FALSE;
            nHomeAllSeq := 0;
            bProgramDone := TRUE;
		END_IF

END_CASE]]></ST>
      </Implementation>
    </Method>
    <Method Name="PRG_HomeX" Id="{1d12177f-1165-4fe4-893e-75362f34ca0e}">
      <Declaration><![CDATA[METHOD PRG_HomeX : BOOL
VAR_INPUT
END_VAR

VAR
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[


CASE nHomeXSeq OF
		 		

	0:
		bProgramDone := FALSE;
		bAxisEnable:=TRUE;
		nHomeXSeq :=10;	
	
	10:
	
		IF X_MC_OUT.bPowerStatus THEN
			
			bHomeX := TRUE;
			nHomeXSeq:=20;
		
		ELSE

			nHomeXSeq:=0;		
		
		END_IF
	
	20:
		IF X_MC_OUT.bAxisHomed THEN
			
			bHomeX:= FALSE;
			nHomeXSeq:=30;
		
		END_IF
		
	30:
		
		bAxisEnable:=FALSE;
		nHomeXSeq:=0;
		bProgramDone := TRUE;



END_CASE]]></ST>
      </Implementation>
    </Method>
