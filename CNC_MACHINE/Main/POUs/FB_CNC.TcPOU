    </Method>
    <Method Name="M_PRG_HomeAll" Id="{dab3d8ef-3b5b-4130-bf86-a872d94b2fd9}">
      <Declaration><![CDATA[METHOD M_PRG_HomeAll : BOOL
VAR_INPUT
END_VAR

VAR

END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[


CASE nHomeAllSeq OF
		 		

	0:
		bProgramDone := FALSE;
		bAxisEnable := TRUE;
        IF X_MC_OUT.bPowerStatus AND X2_MC_OUT.bPowerStatus THEN
            bGearInExecute := TRUE;
            IF X_MC_OUT.bGearInDone THEN
            nHomeAllSeq :=10;	 
		    END_IF
		 END_IF
	10:
		// Home both geared axes first
		IF X_MC_OUT.bPowerStatus AND X2_MC_OUT.bPowerStatus THEN
			bHomeX := TRUE;
			nHomeAllSeq:=20;	
		ELSE
			nHomeAllSeq:=0;
		END_IF
	20:
		IF X_MC_OUT.bAxisHomed THEN
			bHomeX:= FALSE;
            bGearInExecute:=FALSE;
            nHomeAllSeq:=30;
		END_IF
	30:
		bGearOutExecute := TRUE;
		IF X_MC_OUT.bPowerStatus AND X2_MC_OUT.bPowerStatus THEN
            bHomeX := TRUE;
            bHomeX2 := TRUE;
            nHomeAllSeq := 40;
		END_IF
	40:
		IF X_MC_OUT.bAxisHomed THEN
			bHomeX := FALSE;		
		END_IF
        IF X2_MC_OUT.bAxisHomed THEN
            bHomeX2 := FALSE;
		END_IF
        IF X_MC_OUT.bAxisHomed AND X2_MC_OUT.bAxisHomed THEN
            nHomeAllSeq:=50;
		END_IF
		
	50:
	
		bGearOutExecute := FALSE;
        bGearInExecute := TRUE;
        IF X_MC_OUT.bGearInDone THEN
            bAxisEnable := FALSE;
            nHomeAllSeq := 0;
            bProgramDone := TRUE;
		END_IF

END_CASE]]></ST>
      </Implementation>
    </Method>
    <Method Name="M_PRG_HomeX" Id="{1d12177f-1165-4fe4-893e-75362f34ca0e}">
      <Declaration><![CDATA[METHOD M_PRG_HomeX : BOOL
VAR_INPUT
END_VAR

VAR
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[


CASE nHomeXSeq OF
		 		

	0:
		bProgramDone := FALSE;
		bAxisEnable:=TRUE;
        bGearInExecute := TRUE;
		nHomeXSeq :=10;	
	
	10:
	    // Home Both Axes Together
		IF X_MC_OUT.bPowerStatus THEN
			
			bHomeX := TRUE;
			nHomeXSeq:=20;
		
		ELSE

			nHomeXSeq:=0;		
		
		END_IF
	
	20:
		IF X_MC_OUT.bAxisHomed THEN
			
			bHomeX:= FALSE;
			nHomeXSeq:=30;
		
		END_IF
        
    30:
        // Decouple Axes for individual Homing
        
        //X1 Homing
        bGearInExecute := FALSE;
        bGearOutExecute:= TRUE;
        IF X_MC_OUT.bPowerStatus THEN
			bHomeX := TRUE;
			nHomeXSeq:=40;
        ELSE
            bAxisEnable:=TRUE;
            bAxisReset:=TRUE;
            nHomeXSeq:=30;
        END_IF
        
     40:
     
        IF X_MC_OUT.bAxisHomed THEN
			
			bHomeX:= FALSE;
			nHomeXSeq:=50;
		
		END_IF

     50:
        // X2 Homing
       IF X2_MC_OUT.bPowerStatus THEN
			
			bHomeX2 := TRUE;
			nHomeXSeq:=60;
        ELSE
            bAxisEnable:=TRUE;
            bAxisReset:=TRUE;
            nHomeXSeq:=50;
        END_IF
        
      60:
      
        IF X2_MC_OUT.bAxisHomed THEN
			
			bHomeX:= FALSE;
			nHomeXSeq:=70;
		
		END_IF
        

      70:
        bGearOutExecute:=FALSE;
        bGearInExecute:=TRUE;
        IF X_MC_OUT.bGearInDone AND X2_MC_OUT.bGearInDone THEN
            nHomeXSeq:=80;      
		END_IF
        
        
     80:
		
		bAxisEnable:=FALSE;
		nHomeXSeq:=0;
		bProgramDone := TRUE;
        
END_CASE]]></ST>
      </Implementation>
    </Method>
